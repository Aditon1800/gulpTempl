//=== Feedback
section.feedback.sec
  .container
    h2.sec-title Остались вопросы? Закажите обратный звонок!
    .form
      form(method="POST"  target="_self" action='http://shop.advance-club.ru/subscribe/process/?rid[0]=full_sub&doneurl=http%3A%2F%2Fadvance-club.ru%2Fconf_full_sub&activation=1&doneurl2=http%3A%2F%2Fadvance-club.ru%2F/thanks_uni_manager/')
        input.input(type="text" name="lead_name" placeholder="Введите Ваше имя" required)
        input.input.phone-mask(type="text" name="lead_phone" pattern="+7-[0-9]{3}-[0-9]{3}-[0-9]{2}-[0-9]{2}" placeholder="Введите Ваш телефон" required)
        button.btn.feedback__btn Заказать звонок